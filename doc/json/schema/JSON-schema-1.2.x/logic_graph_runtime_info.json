{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "O3D3xx Logic Layer format",
  "type": "object",
  "properties": {
    "connectors": {
      "type":"object",
      "patternProperties": {
        "^C[0-9]+$":   { "type": [ "string", "number", "boolean", "integer" ]}
      }
    },
    "error": {
      "type":"object",
      "properties": {
        "ErrorCode": {"type": "integer"},
        "ErrorMessage": {"type": "string"}
      }
    }
  },
  "required": ["connectors", "error"]
}
